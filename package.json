{
  "name": "nextjs-starter",
  "version": "1.0.0",
  "scripts": {
    "dev": "nvm-guard && next dev",
    "build": "nvm-guard && next build",
    "start": "nvm-guard && next start",
    "lint": "nvm-guard && next lint",
    "stylelint": "nvm-guard && stylelint 'src/**/*.css'",
    "prettier": "nvm-guard && prettier --check 'src/**/*.{ts,tsx,js,jsx}'",
    "prepare": "husky install",
    "type-check": "nvm-guard && tsc"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "next lint --fix --file"
    ],
    "*.css": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.5.0",
    "@commitlint/config-conventional": "17.4.4",
    "@types/node": "18.15.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "install": "0.13.0",
    "lint-staged": "13.2.0",
    "npm": "9.6.2",
    "nvm-guard": "1.0.4",
    "postcss-custom-media": "9.1.2",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "7.8.3",
    "prettier": "2.8.7",
    "stylelint": "15.3.0",
    "stylelint-config-property-sort-order-smacss": "9.1.0",
    "stylelint-config-standard": "31.0.0",
    "typescript": "5.0.2"
  }
}
